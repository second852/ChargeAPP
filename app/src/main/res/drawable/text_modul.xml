<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
          android:state_enabled="false"
        >
        <shape>
            <solid
                android:color="#FFEE99" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
            <corners
                android:radius="7dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <item
        android:state_enabled="true"

        >
        <shape>
            <solid
                android:color="#33CCFF" />
            <stroke
                android:width="1dp"
                android:color="#0044BB" />
            <corners
                android:radius="7dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</selector>


